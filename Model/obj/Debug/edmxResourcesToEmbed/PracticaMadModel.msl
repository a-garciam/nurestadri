      </EntityTypeMapping>
    </EntitySetMapping>
    <AssociationSetMapping Name="Follower" TypeName="PracticaMaDModel.Follower" StoreEntitySet="Follower">
      <EndProperty Name="User">
        <ScalarProperty Name="usrId" ColumnName="followedId" />
      </EndProperty>
