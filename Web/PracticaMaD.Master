<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="PracticaMaD.master.cs" Inherits="Es.Udc.DotNet.PracticaMaD.Web.PracticaMaD" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head id="Head1" runat="server">
    <title>Nurestadri</title>
    <link href="~/Css/styles.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <!-- Window. -->
    <div id="window">
        <!-- Page title. -->
        <div id="pageTitle">
            <asp:Localize ID="lclPageTitle" runat="server" meta:resourcekey="lclPageTitle" />
        </div>
        <!-- Menu. -->
        <div id="menu">
            <span id="menuLinks">

                <asp:ContentPlaceHolder ID="ContentPlaceHolder_MenuLinks" runat="server">
                    <asp:Label ID="lblDash1" runat="server" Text="-" meta:resourcekey="lblDash1Resource1" />
                    <asp:HyperLink ID="lnkAuthenticate" runat="server"
                        NavigateUrl="~/Pages/User/Authentication.aspx">Login</asp:HyperLink>
                    <asp:Label ID="lblDash4" runat="server" Text="-" meta:resourcekey="lblDash1Resource1" />
                    <asp:HyperLink ID="lnkRegister" runat="server"
                        NavigateUrl="~/Pages/User/Register.aspx">Register</asp:HyperLink>
                    <asp:Label ID="lblDash2" runat="server" Text="-" meta:resourcekey="lblDash1Resource1" />
                    <asp:HyperLink ID="lnkUpdate" runat="server"
                        NavigateUrl="~/Pages/User/UpdateUserProfile.aspx">Update Profile</asp:HyperLink>
                    <asp:Label ID="lblDash3" runat="server" Text="-" meta:resourcekey="lblDash1Resource1" />
                    <asp:HyperLink ID="lnkLogout" runat="server"
                        NavigateUrl="~/Pages/User/Logout.aspx">Logout</asp:HyperLink>
                    <asp:Label ID="lblDash7" runat="server" Text="-" meta:resourcekey="lblDash1Resource1" />
                    <asp:HyperLink ID="lnkImageFeed" runat="server"
                        NavigateUrl="~/Pages/Image/ImageFeed.aspx">Images feed</asp:HyperLink>
                    <asp:Label ID="lblDash5" runat="server" Text="-" meta:resourcekey="lblDash1Resource1" />
                    <asp:HyperLink ID="lnkUploadImage" runat="server"
                        NavigateUrl="~/Pages/Image/UploadImage.aspx">Upload Image</asp:HyperLink>
                    <asp:Label ID="lblDash6" runat="server" Text="-" meta:resourcekey="lblDash1Resource1" />
                    <asp:HyperLink ID="lnkUserPhotos" runat="server"
                        NavigateUrl="~/Pages/Image/UserImagesFeed.aspx">Your photos</asp:HyperLink>
                </asp:ContentPlaceHolder>
            </span>
            <br />

            <span id="menuExplanation">
                <asp:ContentPlaceHolder ID="ContentPlaceHolder_MenuExplanation" runat="server">
                </asp:ContentPlaceHolder>
            </span>
        </div>

        <div id="content">
            <asp:ContentPlaceHolder ID="ContentPlaceHolder_BodyContent" runat="server">
            </asp:ContentPlaceHolder>
        </div>
    </div>

    <!-- Footer. -->
    <div id="footer">
        <asp:Localize ID="lclFooter" runat="server" meta:resourcekey="lclFooter" />
    </div>
</body>
</html>